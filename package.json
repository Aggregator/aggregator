{
  "name": "aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "build": "webpack",
    "bithound": "bithound check git@github.com:gdumitrescu/aggregator.git",
    "clean": "rimraf ./coverage",
    "codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "codecov": "codecov",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly",
    "coverage:report": "npm run codacy && npm run codecov && npm run clean",
    "deploy": "surge --project ./public --domain aggregator.surge.sh",
    "format": "standard ./client/**/*.js && standard ./server/**/*.js",
    "karma:unit": "karma start",
    "karma:unit:watch": "npm run karma:unit -- --no-single-run --auto-watch",
    "lambda:build": "netlify-lambda build ./src/lambda",
    "lambda:serve": "netlify-lambda serve ./src/lambda",
    "lint": "eslint --fix ./client/**/*.js && eslint --fix ./server/**/*.js",
    "prestart": "npm run build",
    "start": "node server.js",
    "start:dev": "hjs-dev-server",
    "start:dev:watch": "webpack-dev-server --content-base ./public/ --inline",
    "storybook": "start-storybook -p 9001",
    "swagger:build": "yaml2json ./api/swagger/swagger.yaml > ./api/swagger-ui/swagger.json",
    "test": "mocha client/components/**/*.spec.js",
    "test:e2e": "nightwatch-autorun",
    "test:watch": "npm run test -- --watch"
  },
  "pre-commit": [
    "lint",
    "format",
    "test"
  ],
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdumitrescu/aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdumitrescu/aggregator/issues"
  },
  "homepage": "https://github.com/gdumitrescu/aggregator#readme",
  "dependencies": {
    "chance": "^1.0.13",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "lodash": "^4.17.5",
    "netlify-lambda": "^1.0.0-babel-7-beta",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bithound": "^1.7.0",
    "codacy-coverage": "^2.1.1",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^22.4.3",
    "expect-jsx": "^5.0.0",
    "fetch": "^1.1.0",
    "fetch-mock": "6.3.0",
    "file-loader": "^1.1.11",
    "hjs-webpack": "^9.2.0",
    "html-loader": "^0.5.5",
    "http-server": "^0.11.1",
    "istanbul": "1.1.0-alpha.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.1.0",
    "jsdom": "^11.7.0",
    "karma": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "material-ui": "^0.20.0",
    "materialize-css": "0.100.2",
    "mocha": "^5.0.5",
    "nightwatch-autorun": "^3.1.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.6.2",
    "react-tap-event-plugin": "^3.0.2",
    "rimraf": "^2.6.2",
    "scss-loader": "0.0.1",
    "skin-deep": "^1.2.0",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "surge": "^0.20.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "yamljs": "^0.3.0"
  }
}
