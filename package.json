{
  "name": "aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "build": "webpack",
    "bithound": "bithound check git@github.com:gdumitrescu/aggregator.git",
    "clean": "rimraf ./coverage",
    "codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "codecov": "codecov",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly",
    "coverage:report": "npm run codacy && npm run codecov && npm run clean",
    "deploy": "surge --project ./public --domain aggregator.surge.sh",
    "format": "standard ./client/**/*.js && standard ./server/**/*.js",
    "karma:unit": "karma start",
    "karma:unit:watch": "npm run karma:unit -- --no-single-run --auto-watch",
    "lambda:build": "netlify-lambda build ./src/lambda",
    "lambda:serve": "netlify-lambda serve ./src/lambda",
    "lint": "eslint --fix ./client/**/*.js && eslint --fix ./server/**/*.js",
    "prestart": "npm run build",
    "start": "node server.js",
    "start:dev": "hjs-dev-server",
    "start:dev:watch": "webpack-dev-server --content-base ./public/ --inline",
    "storybook": "start-storybook -p 9001",
    "swagger:build": "yaml2json ./api/swagger/swagger.yaml > ./api/swagger-ui/swagger.json",
    "test": "mocha client/components/**/*.spec.js",
    "test:e2e": "nightwatch-autorun",
    "test:watch": "npm run test -- --watch"
  },
  "pre-commit": [
    "lint",
    "format",
    "test"
  ],
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdumitrescu/aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdumitrescu/aggregator/issues"
  },
  "homepage": "https://github.com/gdumitrescu/aggregator#readme",
  "dependencies": {
    "chance": "^1.0.4",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "lodash": "^4.16.2",
    "netlify-lambda": "^0.2.0",
    "react": "^15.4.0",
    "react-dom": "^15.3.1",
    "whatwg-fetch": "^2.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.28.0",
    "babel-cli": "^6.14.0",
    "babel-core": "6.22.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.16.0",
    "bithound": "^1.7.0",
    "codacy-coverage": "^2.0.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.26.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.1",
    "expect": "^1.20.2",
    "expect-jsx": "^3.0.0",
    "fetch": "^1.1.0",
    "fetch-mock": "5.8.1",
    "file-loader": "^0.10.0",
    "hjs-webpack": "^8.3.0",
    "html-loader": "^0.4.3",
    "http-server": "^0.9.0",
    "istanbul": "1.1.0-alpha.1",
    "istanbul-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "jsdom": "^9.4.2",
    "karma": "^1.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^2.0.2",
    "material-ui": "^0.16.3",
    "materialize-css": "0.98.0",
    "mocha": "^3.0.2",
    "nightwatch-autorun": "^3.0.3",
    "node-sass": "^4.0.0",
    "postcss-loader": "^1.0.0",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.4.0",
    "react-tap-event-plugin": "^2.0.1",
    "rimraf": "^2.5.4",
    "scss-loader": "0.0.1",
    "skin-deep": "^0.16.0",
    "standard": "^8.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "surge": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "yamljs": "^0.2.8"
  }
}
