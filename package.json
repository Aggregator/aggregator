{
  "name": "aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "api:swagger": "yaml2json ./api/swagger/swagger.yaml > ./public/swagger.json",
    "build": "webpack",
    "bithound": "bithound check git@github.com:gdumitrescu/aggregator.git",
    "deploy": "surge --project ./public --domain aggregator.surge.sh",
    "format": "standard ./client/**/*.js && standard ./server/**/*.js",
    "karma:unit": "karma start",
    "karma:unit:watch": "npm run karma:unit -- --no-single-run --auto-watch",
    "lint": "eslint --fix ./client/**/*.js && eslint --fix ./server/**/*.js",
    "prestart": "npm run build",
    "server": "json-server https://raw.githubusercontent.com/gdumitrescu/aggregator/master/db.json --static ./public",
    "start:db": "json-server --watch db.json",
    "start:dev": "hjs-dev-server",
    "start:dev:watch": "webpack-dev-server --content-base ./public/ --inline",
    "start": "node index.js",
    "storybook": "start-storybook -p 9001",
    "test": "mocha client/components/**/*.spec.js",
    "test:e2e": "nightwatch-autorun",
    "test:watch": "npm run test -- --watch"
  },
  "pre-commit": [
    "lint",
    "format",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdumitrescu/aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdumitrescu/aggregator/issues"
  },
  "homepage": "https://github.com/gdumitrescu/aggregator#readme",
  "dependencies": {
    "chance": "^1.0.4",
    "express": "^4.13.4",
    "faker": "^3.1.0",
    "lodash": "^4.13.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.38.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.3",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "bithound": "^1.4.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.1.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.14.0",
    "expect-jsx": "^2.6.0",
    "fetch": "^1.1.0",
    "fetch-mock": "^5.0.1",
    "file-loader": "^0.9.0",
    "hjs-webpack": "^8.3.0",
    "html-loader": "^0.4.3",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jsdom": "^9.4.1",
    "karma": "^1.1.1",
    "karma-coverage": "^1.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.1",
    "materialize-css": "0.97.6",
    "mocha": "^2.5.3",
    "nightwatch-autorun": "^3.0.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.9.1",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "scss-loader": "0.0.1",
    "skin-deep": "^0.16.0",
    "standard": "^7.1.2",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.1",
    "surge": "^0.18.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yamljs": "^0.2.8"
  }
}
