{
  "name": "aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "build": "webpack",
    "deploy": "surge -p ./public -d aggregator.surge.sh",
    "lint": "eslint --fix ./client/**/*.js && eslint --fix ./server/**/*.js",
    "format": "standard ./client/**/*.js && standard ./server/**/*.js",
    "server": "json-server https://raw.githubusercontent.com/gdumitrescu/aggregator/master/db.json --static ./public",
    "prestart": "npm run build",
    "start:db": "json-server --watch db.json",
    "start:dev": "hjs-dev-server",
    "start": "npm run server",
    "test": "mocha client/components/**/*.spec.js",
    "test:watch": "npm run test --  -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdumitrescu/aggregator.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gdumitrescu/aggregator/issues"
  },
  "homepage": "https://github.com/gdumitrescu/aggregator#readme",
  "dependencies": {
    "express": "^4.13.4",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.1.0",
    "eslint-plugin-filenames": "^1.0.0",
    "eslint-plugin-react": "^5.2.2",
    "expect": "^1.14.0",
    "expect-jsx": "^2.6.0",
    "file-loader": "^0.9.0",
    "hjs-webpack": "^8.3.0",
    "html-loader": "^0.4.3",
    "material-ui": "^0.15.1",
    "materialize-css": "0.97.6",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^1.0.0",
    "scss-loader": "0.0.1",
    "skin-deep": "^0.16.0",
